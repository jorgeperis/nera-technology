<div class="header">
  <%= image_tag('cat.png') %>
  <h1>Nera Technology</h1>
</div>
<div class="content">
  <%= form_for :download_excel, url: download_excel_path, format: :xlsx, id: 'files-form' do |f| %>
    <label id="files-label" for="files"><%= image_tag('files.png') %></label>
    <%= f.file_field :files, multiple: true, id: "files", onchange: 'form.submit()', accept: '.XPF' %>
  <% end %>
</div>
